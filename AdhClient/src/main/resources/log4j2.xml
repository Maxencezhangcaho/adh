<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<Configuration>
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{ISO8601} | %-5.5p | %-30.30c{1.} %4L | %m%n"/>
		</Console>
	</Appenders>
	<Loggers>

            <Root level="INFO">
		<AppenderRef ref="console"/>
            </Root>
	</Loggers>
</Configuration>
